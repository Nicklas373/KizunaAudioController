    <!-- Audio Conf String !-->
    <string name="app_menu_1_text">UHQA (Ultra High Quality Audio)</string>
    <string name="app_menu_1_desc">UHQA mode allows to have high performance audio when using headset at primary.</string>
    <string name="app_menu_1_desc_ext">Turn off/on UHQA to switch the amplifier for HPH path.</string>
    <string name="app_menu_1_info">Details</string>
    <string name="app_menu_1_toast_uhqa_scs">Ultra High Quality Audio is active</string>
    <string name="app_menu_1_toast_uhqa_fid">Ultra High Quality Audio is not active</string>
    <string name="app_menu_1_toast_uhqa_ne">UHQA (Ultra High Quality Audio) is not available</string>
    <string name="app_menu_2_text">Headset High Performance Mode</string>
    <string name="app_menu_2_desc">Invoke class AB mode for headset path when sample rate is set to 192KHz and bit depth is set to 24.</string>
    <string name="app_menu_2_desc_ext">Enable it to set headset path in UHQA mode.</string>
    <string name="app_menu_2_toast_hph_scs">Headset High Performance Mode is active</string>
    <string name="app_menu_2_toast_hph_fid">Headset High Performance Mode is not active</string>
    <string name="app_menu_2_toast_hph_ne">Headset High Performance feature is not available</string>
    <string name="app_menu_3_text">Low Distortion AMP</string>
    <string name="app_menu_3_desc">Provide low distortion audio quality by enable/disable AB Class Mode directly on amplifier.</string>
    <string name="app_menu_3_desc_ext">Enable it to configure low distortion mode.</string>
    <string name="app_menu_3_toast_amp_scs">Low Distortion AMP is active</string>
    <string name="app_menu_3_toast_amp_fid">Low Distortion AMP is not active</string>
    <string name="app_menu_3_toast_amp_ne">Low Distortion AMP feature is not available</string>
    <string name="app_menu_4_text">Headphone Impedance Detection</string>
    <string name="app_menu_4_desc">Provide impedance of headphones to be able to adjust gain according to impedance</string>
    <string name="app_menu_4_desc_ext">so user can have consistent volume level with different headphones.</string>
    <string name="app_menu_4_toast_impedance_scs">>Headphone Impedance Detection is active</string>
    <string name="app_menu_4_toast_impedance_fid">Headphone Impedance Detection is not active</string>
    <string name="app_menu_4_toast_impedance_ne">Headphone Impedance Detection feature is not available</string>
    <string name="app_menu_5_text">Experimental Features</string>
    <string name="app_menu_5_desc">NOTE: Experimental features can make problem with audio process</string>
    <string name="app_menu_5_desc_ext">if you are use some service that need exclusive access like BiQuads service or other.</string>
    <string name="app_menu_5_toast_ef_scs">Experimental Features is active</string>
    <string name="app_menu_5_toast_ef_fid">Experimental Features is not active</string>
    <string name="app_menu_6_text">Qualcomm Power Gating</string>
